  "la6000d": {
    "xpathExpression": "//div[@class=\"course\" and .//a[normalize-space(text())=\"%s\"]]",
    "elements": [{
      "name": "name",
      "type": "string",
      "node": "h2 > a"
    }, {
      "name": "distance",
      "type": "number",
      "node": "ul > li:first-child"
    }, {
